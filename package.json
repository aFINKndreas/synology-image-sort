{
  "name": "synology-image-sort",
  "version": "0.0.9",
  "author": "Andreas Fink",
  "bin": {
    "synology-image-sort": "./bin/run"
  },
  "bugs": "https://github.com/afinkndreas/synology-image-sort/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/fs-extra": "^9.0.11",
    "@types/listr": "^0.14.3",
    "@types/node": "^15.0.3",
    "@types/recursive-readdir": "^2.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/afinkndreas/synology-image-sort",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "synology-image-sort",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "afinkndreas/synology-image-sort",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chokidar": "^3.5.1",
    "cli-ux": "^5.5.1",
    "date-fns": "^1.30.1",
    "exiftool-kit": "^0.1.8",
    "fast-exif": "^1.0.1",
    "fs-extra": "^8.1.0",
    "listr": "^0.14.3",
    "recursive-readdir": "^2.2.2",
    "tslib": "^1.14.1"
  }
}
